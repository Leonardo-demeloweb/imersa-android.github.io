<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>IMERSA</name>
    <description>
        Angular-Android-App
    </description>
    <author email="demelo01@gmail.com" href="http://www.leonardo-desenvolvedor.surge.sh">
        YKS-Sistemas
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <platform name="android">
        <icon density="ldpi" src="assets/img/imersa-36.png" />
        <icon density="mdpi" src="assets/img/imersa-48.png" />
        <icon density="hdpi" src="assets/img/imersa-72.png" />
        <icon density="xhdpi" src="assets/img/imersa-96.png" />
    </platform>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
</widget>
